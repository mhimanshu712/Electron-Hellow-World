{
    "manifest_version": 2,
    "name": "Simple To Do List",
    "version": "1.0",
    "description": "A simple and minimal to-do list, no ads no signup.",
    "browser_action":{
        "default_popup": "home.html"
    },
    "icons":{
        "16":"icons/list16.png",
        "48":"icons/list48.png",
        "128":"icons/list128.png"
    },
    "permissions": ["activeTab"]
    
}